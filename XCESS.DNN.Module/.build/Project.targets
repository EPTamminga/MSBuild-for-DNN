<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!-- Imports and Setup: Do not modify! -->
  <PropertyGroup>
    <DnnMSBuildTasksPath>$(SolutionDir)\Dnn.MsBuild.Tasks</DnnMSBuildTasksPath>
    <MSBuildCommunityTasksPath>$(SolutionDir)\.build</MSBuildCommunityTasksPath>
  </PropertyGroup>

  <Import Project="$(DnnMSBuildTasksPath)\Dnn.MsBuild.Tasks.targets" />

  <!-- Project Settings: can be modified, but the defaults should be just fine! -->
	<PropertyGroup>
		<BinFolder>$(MSBuildProjectDirectory)\bin</BinFolder>
		<DnnManifestExtension>dnn</DnnManifestExtension>
		<PackageFolder>packages</PackageFolder>
		<PackageExtension>zip</PackageExtension>
	</PropertyGroup>
</Project>